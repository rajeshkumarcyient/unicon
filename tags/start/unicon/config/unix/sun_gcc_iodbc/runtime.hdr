GDBM=gdbm
LIBTP=libtp
CC=gcc
# tell where myodbc has been installed, e.g. MYODBC=../myodbc; default empty
MYODBC=
CFLAGS= -O -I../xpm -I../libtp -I$(MYODBC)/usr/local/include -I/usr/openwin/include
LDFLAGS=-ldl
LIBS=-lm -lXpm -lgdbm -ltp -lsocket -lnsl -L$(MYODBC)/usr/local/lib -liodbc
SHELL=/bin/sh
